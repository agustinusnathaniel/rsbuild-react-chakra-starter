{
  "name": "rsbuild-react-chakra-starter",
  "version": "0.1.0",
  "author": "sozonome",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/plus-jakarta-sans": "^5.0.20",
    "framer-motion": "^11.2.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.1",
    "web-vitals": "^4.0.1"
  },
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build",
    "build-and-analyze": "cross-env RSDOCTOR=true pnpm build",
    "preview": "rsbuild preview",
    "lint": "biome lint ./src",
    "toolchain:fix": "biome check --apply ./src rsbuild.config.ts commitlint.config.js",
    "type-check": "tsc --noEmit",
    "check:turbo": "pnpm dlx turbo@latest lint type-check",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@rsbuild/core": "^0.7.1",
    "@rsbuild/plugin-assets-retry": "^0.7.1",
    "@rsbuild/plugin-image-compress": "^0.7.1",
    "@rsbuild/plugin-react": "^0.7.1",
    "@rsbuild/plugin-svgr": "^0.7.1",
    "@rsbuild/plugin-type-check": "^0.7.1",
    "@rsdoctor/rspack-plugin": "^0.2.5",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "standard-version": "^9.5.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.20.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.1"
}
